    function onload() {
        console.log("onload function calling .....");
        var timeonload = setInterval(function() {
            jQuery('.current_price').each(function() {
                if ($(this).hasClass("bundletag")) {
                    var matvaltag = $(this).attr("data-price-without");
                    var matvalper = $(this).attr("data-price-per");
                    //console.log(parseFloat(matvaltag).toFixed(2));
                    if (matvaltag == undefined) {
                        $(this).closest(".custom_main_price").find(".custom_sale_price").html("Free");
                    } else {
                        clearInterval(timeonload);
                        var percentage = matvaltag*matvalper/100;
                        var percentage = parseFloat(percentage).toFixed(3);	
                        var mainFinal = matvaltag - percentage;	
                        var mainFinal = mainFinal.toFixed(2);
                        var mainFinal = Shopify.formatMoney(mainFinal);
                        $(this).empty();
                        $(this).html(mainFinal);
                        var final = matvaltag;
                        var final = parseFloat(final).toFixed(2);
                        var final = Shopify.formatMoney(final);
                        console.log(final);
                        var html = '<span class="was_price" style="padding-left: 3px;">'+final+'</span>';
                        $(this).after(html);
                    }
                }
            });
        }, 1000);
    }

    var Shopify = Shopify || {};
    Shopify.money_format = {{ shop.money_format | json }};
    Shopify.formatMoney = function(cents, format) {
        if (typeof cents == 'string') {
            cents = cents.replace('.', '');
        }
        var value = '';
        var placeholderRegex = /\{\{\s*(\w+)\s*\}\}/;
        var formatString = (format || this.money_format);
    
        function defaultOption(opt, def) {
            return (typeof opt == 'undefined' ? def : opt);
        }
    
        function formatWithDelimiters(number, precision, thousands, decimal) {
            precision = defaultOption(precision, 2);
            thousands = defaultOption(thousands, ',');
            decimal = defaultOption(decimal, '.');
    
            if (isNaN(number) || number == null) {
                return 0;
            }
    
            number = (number / 100.0).toFixed(precision);
    
            var parts = number.split('.'),
                dollars = parts[0].replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1' + thousands),
                cents = parts[1] ? (decimal + parts[1]) : '';
    
            return dollars + cents;
        }
    
        switch (formatString.match(placeholderRegex)[1]) {
            case 'amount':
                value = formatWithDelimiters(cents, 2);
                break;
            case 'amount_no_decimals':
                value = formatWithDelimiters(cents, 0);
                break;
            case 'amount_with_comma_separator':
                value = formatWithDelimiters(cents, 2, '.', ',');
                break;
            case 'amount_no_decimals_with_comma_separator':
                value = formatWithDelimiters(cents, 0, '.', ',');
                break;
        }
        return formatString.replace(placeholderRegex, value);
    };
  </script>

